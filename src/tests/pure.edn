(defpure upload-proto
  (ExpectBytes)
  "Hello" = .proto-data
  "Namaste")

(defwire main
  "Proto-Indo-European" = .proto-data
  .proto-data (ToBytes) (Do upload-proto) (Log)
  .proto-data (Log)
  (Assert.Is "Proto-Indo-European" true))

(defmesh root)

(schedule root main)
(if (run root 0.1) nil (throw "Root tick failed"))